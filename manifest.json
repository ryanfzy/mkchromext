{
	"manifest_version": 2,
	"name": "my extension",
	"version": "0.1",
	"description": "douban search",
	"browser_action": {
		"default_icon": "search.png",
		"default_title": "my ext",
		"default_popup": "popup.html"
	},
    "background": {
        "scripts": ["bg.js"]
    },
    "content_scripts": [
        {
            "matches": ["http://www.66ys.cc/*/*"],
            "js": ["jquery-1.11.1.min.js", "cs.js"],
            "run_at": "document_end"
        }
    ],
	"permissions": [
		"tabs",
		"storage",
		"http://*/",
		"https://*/"
	],
    "web_accessible_resources" : ["search.png"]
}
